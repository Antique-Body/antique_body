{
  "name": "antique_body",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate && prisma migrate deploy && prisma db seed",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,css,scss,md,html,yml,yaml}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json,css,scss,md,html,yml,yaml}'",
    "format:all": "npm run lint:fix && npm run format",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run format:check && npm run type-check",
    "fix": "npm run lint:fix && npm run format",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:create": "npx prisma migrate dev --name",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@google-cloud/storage": "^7.16.0",
    "@iconify/react": "^6.0.0",
    "@prisma/client": "^6.10.1",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.0",
    "formidable": "^3.5.4",
    "framer-motion": "^12.19.2",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "next": "^15.3.4",
    "next-auth": "^5.0.0-beta.28",
    "nodemailer": "^6.9.12",
    "prisma": "^6.10.1",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-i18next": "^15.5.3",
    "sass": "^1.89.2",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "eslint": "^9.30.0",
    "eslint-config-next": "^15.3.4",
    "tailwindcss": "^4.1.11"
  }
}
